{
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["ESNext"],
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"types": ["@cloudflare/workers-types"],
		"strict": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src"]
}
